<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:oxy="clr-namespace:OxyPlot.Avalonia;assembly=OxyPlot.Avalonia"
        x:Class="GUI.Views.MainWindow"
        Width="600" Height="400" Title="Maths Interpreter">
    <StackPanel Margin="10">
        <TextBox Name="InputExpression" Watermark="Enter Mathematical Expression:" />
        <Button Content="Evaluate" Click="CalculateOnClick" Margin="0,10,0,10"/>
        <TextBox Name="OutputResult" IsReadOnly="True" Watermark="Result" />
        <Button Content="Plot" Click="PlotOnClick" Margin="0,10,0,10"/>
        <oxy:PlotView Name="plotView1" Width="300" Height="200"/>
    </StackPanel>
</Window>
